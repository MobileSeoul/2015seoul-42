<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nodedata.project.subway" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:theme="@android:style/Theme.Black.NoTitleBar" />
        <activity android:name=".DetailMainActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".BookmarkActivity" android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".StationTimeActivity" android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".DetailViewActivity" android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".TransferRouteActivity" android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".ResultPahAcitivity" android:theme="@android:style/Theme.Translucent.NoTitleBar" />

    </application>

</manifest>
